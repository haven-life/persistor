{
    "name": "persistor",
    "description": "A subclass of supertype that serializes to and reconstitutes from mongodb",
    "homepage": "https://github.com/haven-life/persistor",
    "version": "2.3.14",
    "dependencies": {
        "q": "1.x",
        "supertype": "2.2.*",
        "underscore": "1.x",
        "mongodb-bluebird": "x",
        "bluebird": "x",
        "knex": "0.13.0",
        "pg": "7.4.1",
        "tv4": "^1.2.7"
    },
    "devDependencies": {
        "chai": "3.x",
        "chai-as-promised": "^5.1.0",
        "istanbul": "0.4.5",
        "mocha": "5.2.0",
        "sinon": "1.15.x",
        "sinon-chai": "2.8.x",
        "mock-fs": "3.6.x",
        "eslint": "3.7.x",
        "@types/chai": "^3.4.34",
        "@types/mocha": "^2.2.39",
        "@types/node": "^7.0.5",
        "@types/q": "*",
        "@types/underscore": "*",
        "@types/bluebird": "*",
        "typescript": "*",
        "ts-node": "*"
    },
    "directories": {},
    "engines": {
        "node": ">=0.8.0"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/haven-life/persistor.git"
    },
    "scripts": {
        "lint": "eslint .",
        "coverage": "istanbul cover mocha --exit -- test/* -R spec",
        "test": "istanbul cover _mocha -- test/* -R spec",
        "test:debug": "mocha test/**/*.js --inspect-brk=0.0.0.0:5858",
        "test:docker": "docker-compose up --build",
        "test:docker:debug": "docker-compose -f docker-compose-debug.yml up --build",
        "test:compiled:js": "mocha test/supertype/persist_banking_pgsql.js",
        "test:config": "mocha test/config",
        "test:example": "mocha test/example",
        "test:ts": "mocha --compilers ts:ts-node/register test/supertype/persist_banking_pgsql.ts",
        "compile": "tsc && cp index.js dist/ && cp -R lib dist/"
    },
    "bugs": {
        "url": "http://github.com/haven-life/persistor/issues"
    },
    "optionalDependencies": {}
}
