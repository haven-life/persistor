{
    "name": "persistor",
    "description": "A subclass of supertype that serializes to and reconstitutes from mongodb",
    "homepage": "https://github.com/haven-life/persistor",
    "version": "2.8.3",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "dependencies": {
        "bluebird": "x",
        "knex": "0.15.2",
        "mongodb-bluebird": "x",
        "pg": "7.4.1",
        "q": "1.x",
        "supertype": "github:haven-life/supertype#bug/fixing-decorator-supertypeclass",
        "tv4": "^1.2.7",
        "underscore": "1.x",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@types/bluebird": "*",
        "@types/chai": "^3.4.34",
        "@types/knex": "^0.15.1",
        "@types/mocha": "^2.2.39",
        "@types/mongodb": "^3.1.19",
        "@types/node": "^7.0.5",
        "@types/q": "*",
        "@types/underscore": "*",
        "@types/uuid": "^3.4.4",
        "chai": "3.x",
        "chai-as-promised": "^5.1.0",
        "decache": "^4.5.1",
        "eslint": "3.7.x",
        "mocha": "5.2.0",
        "mock-fs": "3.6.x",
        "nyc": "13.0.0",
        "sinon": "1.15.x",
        "sinon-chai": "2.8.x",
        "ts-node": "*",
        "typescript": "*"
    },
    "directories": {},
    "engines": {
        "node": ">=0.8.0"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/haven-life/persistor.git"
    },
    "scripts": {
        "lint": "eslint .",
        "coverage": "nyc mocha --exit -- test/*",
        "compile": "tsc -p ./tsconfig.json",
        "compile:tests": "tsc -p ./test/supertype/tsconfig.test.json",
        "compile:all": "npm run compile && npm run compile:tests",
        "prepublishOnly": "npm run compile",
        "test": "mocha --require source-map-support/register  test/persist_newapi_tests.js --exit",
        "test:debug": "mocha test/persist_banking_pgsql.js --inspect-brk=0.0.0.0:5858",
        "test:docker": "docker-compose up --build",
        "test:docker:debug": "docker-compose -f docker-compose-debug.yml up --build",
        "restart:docker:debug": "docker-compose -f docker-compose-debug.yml restart && docker-compose logs -f --tail 100 persistor-test",
        "restart:docker": "docker-compose -f docker-compose.yml restart && docker-compose logs -f --tail 100 persistor-test",
        "test:compiled:js": "_mocha ./test/supertype/persist_banking_pgsql.js --exit",
        "test:config": "mocha test/config --exit",
        "test:example": "mocha test/example --exit",
        "test:ts": "mocha --compilers ts:ts-node/register test/supertype/persist_banking_pgsql.ts",
        "test:ts:debug": "mocha --inspect-brk --compilers ts:ts-node/register test/supertype/persist_banking_pgsql.ts",
        "compile:watch": "tsc -w -p ./tsconfig.json"
    },
    "bugs": {
        "url": "http://github.com/haven-life/persistor/issues"
    },
    "optionalDependencies": {}
}
